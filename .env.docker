# Sonote Docker Environment Variables
# Copy to .env and configure before running docker-compose

# ========== Mock Mode ==========
USE_MOCK=false

# ========== DashScope (Aliyun) ==========
DASHSCOPE_API_KEY=sk-your-dashscope-api-key

# ========== DeepSeek (Optional) ==========
DEEPSEEK_API_KEY=
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# ========== LLM Provider ==========
LLM_PROVIDER=dashscope
